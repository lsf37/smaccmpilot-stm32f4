<?xml version="1.0" encoding="UTF-8" ?>
<system>
  <modules>
    <module name="smaccmpilot.build" />
    <module name="armv7m.ctxt-switch-preempt" />

    <module name="armv7m.exception-preempt">
      <trampolines>

         <!-- the interrupt handlers that can cause preemption:-->

        <trampoline>
          <name>systick</name>
          <handler>echronos_systick</handler>
        </trampoline>

        <trampoline>
          <name>tim1_up_tim10</name>
          <handler>eChronos_TIM1_UP_TIM10_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>tim1_cc</name>
          <handler>eChronos_TIM1_CC_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>i2c2_ev</name>
          <handler>eChronos_I2C2_EV_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>i2c2_er</name>
          <handler>eChronos_I2C2_ER_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>spi1</name>
          <handler>eChronos_SPI1_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>usart1</name>
          <handler>eChronos_USART1_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>usart2</name>
          <handler>eChronos_USART2_IRQHandler</handler>
        </trampoline>
        <trampoline>
          <name>uart5</name>
          <handler>eChronos_UART5_IRQHandler</handler>
        </trampoline> 
        <trampoline>
          <name>usart6</name>
          <handler>eChronos_USART6_IRQHandler</handler>
        </trampoline> 

      </trampolines>
    </module>

    <module name="smaccmpilot.vectable">
        <flash_load_addr>0x08000000</flash_load_addr>
        <code_addr>0x08000000</code_addr>
        <data_addr>0x20000000</data_addr>
        <stack_size>0x800</stack_size>

      <!-- add wrapped systick irq handler to vectable: -->
      <systick>rtos_internal_exception_preempt_trampoline_systick</systick>
      <svcall>rtos_internal_svc_handler</svcall>
      <pendsv>rtos_internal_pendsv_handler</pendsv>

      <external_irqs>
        <external_irq>
          <number>25</number>
          <handler>rtos_internal_exception_preempt_trampoline_tim1_up_tim10</handler>
        </external_irq>
        <external_irq>
          <number>27</number>
          <handler>rtos_internal_exception_preempt_trampoline_tim1_cc</handler>
        </external_irq>
        <external_irq>
          <number>33</number>
          <handler>rtos_internal_exception_preempt_trampoline_i2c2_ev</handler>
        </external_irq>
        <external_irq>
          <number>34</number>
          <handler>rtos_internal_exception_preempt_trampoline_i2c2_er</handler>
        </external_irq>
        <external_irq>
          <number>35</number>
          <handler>rtos_internal_exception_preempt_trampoline_spi1</handler>
        </external_irq>
        <external_irq>
          <number>37</number>
          <handler>rtos_internal_exception_preempt_trampoline_usart1</handler>
        </external_irq>
        <external_irq>
          <number>38</number>
          <handler>rtos_internal_exception_preempt_trampoline_usart2</handler>
        </external_irq>
        <external_irq>
          <number>53</number>
